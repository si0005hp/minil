def bubblesort(data)
	i = data.length - 1
	cycle = 0

	while cycle < data.length do

		while i - 1 >= cycle do
			r = data[i]
			l = data[i - 1]

			if r < l
				swap(data, i, i - 1)
			end
			i = i - 1
		end

		cycle = cycle + 1
		i = data.length - 1
	end

	return data
end

def swap(arr, xi, yi)
	tmp = arr[xi]
	arr[xi] = arr[yi]
	arr[yi] = tmp
end



data = [1,9,2,7,4]
bubblesort(data)

